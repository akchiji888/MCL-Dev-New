                        <TabItem  Header="Mod安装" Height="68.57">
                            <Grid>
                                <TextBlock HorizontalAlignment="Left" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF33B4F5" FontSize="16" Text="搜索Mod："/>
                                <Button puthe:ButtonStylist.Style="ColorBorder" BorderThickness="1" Content="&#xf002;" FontFamily="{DynamicResource Awesome}" x:Name="ModSearch" pu:ButtonHelper.CornerRadius="5,5,5,5" Margin="304,33,0,0" FontSize="18"  Width="55" Height="36" Click="ModSearch_Click" BorderBrush="{x:Null}"/>
                                <TextBox Width="200" x:Name="ModInput" pu:TextBoxHelper.CornerRadius="5" pu:TextBoxHelper.Watermark="请输入Mod的名字(仅支持英语)" puthe:TextBoxStylist.Style="Border" Margin="99,34,333,0" />
                                <DataGrid SelectionMode="Single" Visibility="Hidden" MouseDoubleClick="ModGrid_MouseDoubleClick" CanUserAddRows="False" CanUserDeleteRows="False" CanUserSortColumns="False" CanUserResizeColumns="False" CanUserReorderColumns="False" puthe:DataGridStylist.Style="Solid" AutoGenerateColumns="False" x:Name="ModGrid" Margin="0,74,0,0">
