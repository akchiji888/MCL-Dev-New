                                <TextBlock HorizontalAlignment="Left" Margin="10,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF33B4F5"><Run Language="zh-cn" Text="角色:"/></TextBlock>
                                <ComboBox DisplayMemberPath="Name" x:Name="players" HorizontalAlignment="Left" Margin="45,115,0,0" Width="133" Height="22"/>
                            </Grid>
                        </TabItem>
                        <TabItem  Width="97" pu:TabItemHelper.Icon="&#xf17a;" pu:IconHelper.FontFamily="{DynamicResource Awesome}" Header="微软登录">
